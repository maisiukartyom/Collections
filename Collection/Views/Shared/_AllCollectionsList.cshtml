@model List<MCollection>

@foreach(var obj in Model)
            {
                <tr>
                    <td width="5%">
                        <a asp-route-id="@obj.Name" asp-action="Collections">@obj.Name</a>
                    </td>
                    <td width="15%">
                        @obj.Theme
                    </td>
                    <td width="10%">
                        @obj.Owner
                    </td> 
                    <td width="5%">
                        @obj.Size
                    </td> 
                </tr>
            }